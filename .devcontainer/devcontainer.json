{
  "name": "Exam Env",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "-GitHub.copilot",
        "-GitHub.copilot-chat",
        "-GitHub.copilot-workspace",
        "-Continue.continue",
        "-Sourcegraph.cody-ai",
        "-TabNine.tabnine-vscode",
        "-AWS.amazon-q-vscode"
      ],
      "settings": {
        "github.copilot.enable": { "*": false },
        "github.copilot.chat.enabled": false,
        "editor.inlineSuggest.enabled": false,
        "chat.commandCenter.enabled": false,
        "chat.editor.enable": false,
        "chat.experimental.showWelcome": false,
        "workbench.startupEditor": "none"
      }
    }
  },
  "postCreateCommand": "for ext in GitHub.copilot GitHub.copilot-chat GitHub.copilot-workspace Continue.continue Sourcegraph.cody-ai TabNine.tabnine-vscode AWS.amazon-q-vscode; do code --uninstall-extension $ext || true; done"
}
